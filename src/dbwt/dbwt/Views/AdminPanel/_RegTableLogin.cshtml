@{
}


@model IEnumerable<dbwt.Models.user_table>

<h2> LETZTE REGISTRIERUNGEN </h2>
<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">TIMESTAMP</th>
            <th scope="col">Loginname (EMAIL)</th>
            <th scope="col">ACTION</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.time</td>
                <td><a href="#" title="ID: @item.ID ROLE: @item.role">@item.Email</a></td>
                <td><form action="#" method="post"><input type="hidden" name="id" value="@item.ID" /><input type="hidden" name="action" value="disbale" /><input type="submit" value="SPERREN"/></form></td>

            </tr>

        }

    </tbody>
</table>

